
func REAL fibo(REAL n){

	escribe "valor de n en la llamada ";
	escribe n;
	escribe "\n";
	
	si (n == 0){
		n = 0 + 0;
	}
	si (n == 1){
		n = n + 0;
	}

	si (n > 1){
		REAL a = 0;
		REAL b = 0;
		a = n - 1;
		b = n - 2;
		b = llama fibo(REAL b);
		a = llama fibo(REAL a);
		n = a  + b ;
	}		
	devuelve n;
}


func inicio{
